{
  "name": "@remrob/api",
  "version": "0.2.0",
  "description": "API of the REMROB ecosystem",
  "keywords": [
    "remrob",
    "service",
    "api"
  ],
  "homepage": "https://bitbucket.org/remrob/r2ecosystem",
  "repository": "https://bitbucket.org/remrob/r2ecosystem",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Emir Bakhtarov",
    "email": "bakhtarov@remrob.com"
  },
  "contributors": [
    {
      "name": "Muslim Idris",
      "email": "gelamu@gmail.com"
    }
  ],
  "main": "src/server.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "bun run clean; tsc -b",
    "clean": "rm -rf dist node_modules ../../.build-cache/services-api.tsbuildinfo",
    "prepack": "bun run build",
    "prepare": "ts-patch install && typia patch",
    "sort-json-files": "sortjson *.json && prettier -w *.json",
    "start": "bun run --env-file=../../.env --watch src/server.ts"
  },
  "dependencies": {
    "@remrob/aws": "workspace:*",
    "@remrob/drizzle": "workspace:*",
    "@remrob/log": "workspace:*",
    "@remrob/redis": "workspace:*",
    "@remrob/shuttle": "workspace:*",
    "@remrob/t800": "workspace:*",
    "@remrob/utils": "workspace:*",
    "@ryoppippi/unplugin-typia": "npm:@jsr/ryoppippi__unplugin-typia",
    "@trpc/server": "next",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.33.0",
    "express": "^4.19.2",
    "express-bearer-token": "^2.4.0",
    "extract-zip": "^2.0.1",
    "helmet": "^3.23.3",
    "js-base64": "^2.6.4",
    "mime": "^2.6.0",
    "request": "^2.88.2",
    "serialize-error": "^2.1.0",
    "superjson": "^2.2.1",
    "typia": "^6.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bun": "^1.1.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "0.0.43",
    "@types/mime": "^2.0.3",
    "@types/node": "^20",
    "@types/redis": "^2.8.32",
    "@types/request": "^2.48.12",
    "@types/uuid": "^8.3.4",
    "json-sort-cli": "^1",
    "prettier": "^2",
    "supertest": "^3.4.2",
    "ts-patch": "^3.2.1",
    "typescript": "^5.5.4"
  }
}
