  "scripts": {
    "build": "export SKIP_PREFLIGHT_CHECK=true; yarn react-scripts build",
    "clean": "yarn rimraf build",
    "eject": "yarn react-scripts eject",
    "prepack": "yarn build",
    "sort-json-files": "yarn sortjson *.json && yarn prettier -w *.json",
    "start": "BROWSER=none yarn react-scripts start",
    "test": "yarn react-scripts test --coverage --silent=false"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
